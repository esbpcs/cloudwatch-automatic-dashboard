{
    "development": {
        "Name": "AutomatedCloudWatch-Dev",
        "DashboardName": "Dev-Dashboard",
        "MonitoringTagKey": "ManagedBy",
        "MonitoringTagValue": "esbpcs-dev",
        "SLOTargetPercentage": 99.9,
        "ScheduleExpression": "rate(1 day)",
        "LambdaMemory": 512,
        "LambdaTimeout": 900,
        "LambdaCodeS3Bucket": "esbpcs-cloudwatch-dashboard-artifacts",
        "LambdaCodeS3Key": "function.zip",
        "ssm_parameter_for_role": "/github/dashboard/development/role-arn"
    }
